<block wx:if="{{$taroCompReady}}">
    <view class="searchBar">
        <block>
            <block wx:if="{{showType == 1}}">
                <view class="inputBox">
                    <image bindtap="anonymousFunc0" src="{{_$anonymousState__temp}}"></image>
                    <input placeholder="请输入搜索的内容" />
                </view>
            </block>
            <block wx:else>
                <view bindtap="anonymousFunc1" class="searchBtn">
                    <image src="{{_$anonymousState__temp2}}"></image>
                </view>
            </block>
        </block>
    </view>
</block>